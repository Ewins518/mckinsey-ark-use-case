{
  "mcpProxy": {
    "addr": ":9090",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "type": "sse",
    "options": {
      "panicIfInvalid": true,
      "logEnabled": true
    }
  },
  "mcpServers": {
    "filesys": {
      "command": "/bin/sh",
      "args": [
        "-c",
        "cd /data; mcp-filesystem-server /data"
      ]
    }
  }
}